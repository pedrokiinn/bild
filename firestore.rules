rules_version = '2';

service cloud.firestore {
  match /databases/{database}/documents {
    // WARNING: These rules are insecure and allow anyone to read and write to your database.
    // They are intended for initial development and debugging.
    // You should implement secure rules before deploying to production.
    match /{document=**} {
      allow read, write: if true;
    }
  }
}
